  "get_res_num_rules": [
    {
      "expression": "^A-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "regNum",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^A-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "otherNum",
      "otherType": "索书号",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^H-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "cateNum",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^I-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "oldNum",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^J-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "comeNum",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^K-(?P<num>[\\da-zA-Z]+)[_-].*$",
      "field": "collectionNum",
      "order": 1,
      "enable": true,
      "description": "说明"
    },
    {
      "expression": "^(?P<num>[\\da-zA-Z]+)[\\_\\-](?P<name>.+)$",
      "field": "oldNum",
      "order": 7,
      "enable": true,
      "description": "说明"
    }
  ],
